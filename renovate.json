{
  "extends": ["config:base"],
  "platformAutomerge": true,
  "allowedPostUpgradeCommands": ["./bin/update-cypress"],
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "automerge": true
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ]
}
