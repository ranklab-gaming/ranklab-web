{
  "private": true,
  "keywords": [
    "turbo"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "codegen": "./bin/codegen",
    "test": "NODE_ENV=test turbo run build test"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "turbo": "1.0.4"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          ".next/**"
        ]
      },
      "test": {
        "dependsOn": [
          "build"
        ],
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    }
  }
}
