{
  "compilerOptions": {
    "target": "ES2016",
    "module": "Node16",
    "rootDir": "./src",
    "moduleResolution": "node16",
    "baseUrl": "./src",
    "declaration": true,
    "inlineSourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
