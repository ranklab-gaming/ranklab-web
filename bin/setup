#!/bin/sh

rootdir=$(dirname "$(readlink -f "$0")")/..

cd $rootdir
sudo chown -R vscode .
aws s3 cp s3://ranklab-development-secrets/.env.web ./packages/ranklab-web/.env.development.local
aws s3 cp s3://ranklab-test-secrets/.env.web ./packages/ranklab-web/.env.test.local --profile test
yarn install
yarn workspace @ranklab/api build
