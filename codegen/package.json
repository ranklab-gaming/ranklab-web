{
  "name": "@ranklab/api",
  "version": "1.0.0",
  "description": "swagger client for @ranklab/api",
  "author": "Ranklab Ltd",
  "keywords": [
    "fetch",
    "typescript",
    "swagger-client",
    "@ranklab/api"
  ],
  "license": "Unlicense",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist/",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "jest": "^27.2.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
